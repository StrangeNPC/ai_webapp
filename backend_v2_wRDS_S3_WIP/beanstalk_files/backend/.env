OPENAI_API_KEY=your_openai_api_key_here
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1 

# S3 Configuration
S3_BUCKET_NAME=ai-webapp-uploaded-files # Your bucket name

# RDS Database Configuration (Example for PostgreSQL)
DB_TYPE=postgresql # e.g., postgresql, mysql
DB_DRIVER=psycopg2 # e.g., psycopg2, mysqlconnector
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=your_rds_endpoint_hostname # e.g., my-db-instance.xxxxxxxx.us-east-1.rds.amazonaws.com
DB_PORT=5432 # Default for PostgreSQL
DB_NAME=your_db_name